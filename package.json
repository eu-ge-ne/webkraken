{
  "name": "webkraken",
  "version": "0.0.20",
  "description": "web crawler",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eu-ge-ne/webkraken.git"
  },
  "author": "Eugene Pobochny <eugene.pobochny@gmail.com>",
