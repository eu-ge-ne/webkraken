    "prettier": "2.7.1",
    "rimraf": "3.0.2",
    "typescript": "4.8.2"
  },
  "dependencies": {
    "better-sqlite3": "7.6.2",
    "chalk": "5.0.1",
    "commander": "9.4.0",
    "figures": "5.0.0",
    "got": "12.4.1",
    "hpagent": "1.0.0",
    "linkedom": "0.14.12",
    "pretty-ms": "8.0.0",
    "string-width": "5.1.2",
    "user-agents": "1.0.1130"
